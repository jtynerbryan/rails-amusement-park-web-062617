<%= form_for @user  do |f|%>
<label>Name</label><br>
<%= f.text_field :name %><br>

<label>Height</label><br>
<%= f.text_field :height %><br>

<label>How happy are you? 1-5</label><br>
<%= f.text_field :happiness %><br>

<label>How nauseous are you? 1-5</label><br>
<%= f.text_field :nausea %><br>

<label>How many tickets do you have</label><br>
<%= f.text_field :tickets %><br>

<label>Are you a park owner</label>
<%= f.check_box :admin %><br>

Password: <%= f.password_field :password %><br>

<%= f.submit %>

<% end %>